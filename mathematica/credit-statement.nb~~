Cell[CellGroupData[{

Cell["Helper Functions", "Chapter",
 CellChangeTimes->{{3.824481854798043*^9, 3.8244818561261697`*^9}}],

Cell[CellGroupData[{

Cell["EditToExportURL", "Section",
 CellChangeTimes->{{3.8244819497825937`*^9, 3.824481951984687*^9}}],

Cell[BoxData[{
 RowBox[{"Clear", "@", "EditToExportURL"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"EditToExportURL", "[", "url_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Append", "[", 
      RowBox[{
       RowBox[{"KeyDrop", "[", 
        RowBox[{"#", ",", 
         RowBox[{"{", 
          RowBox[{
          "\"\<Path\>\"", ",", "\"\<Query\>\"", ",", "\"\<Fragment\>\""}], 
          "}"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\"\<Path\>\"", "\[Rule]", 
          RowBox[{"StringReplace", "[", 
           RowBox[{
            RowBox[{"#", "[", "\"\<Path\>\"", "]"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"#", "[", "\"\<Path\>\"", "]"}], "\[LeftDoubleBracket]", 
              RowBox[{"-", "1"}], "\[RightDoubleBracket]"}], "\[Rule]", 
             "\"\<export\>\""}]}], "]"}]}], ",", 
         RowBox[{"\"\<Query\>\"", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               "\[Rule]", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
              "&"}], "@", 
             RowBox[{"StringSplit", "[", 
              RowBox[{
               RowBox[{"#", "[", "\"\<Fragment\>\"", "]"}], ",", 
               "\"\<=\>\""}], "]"}]}], ",", 
            RowBox[{"\"\<format\>\"", "\[Rule]", "\"\<csv\>\""}]}], "}"}]}], 
         ",", 
         RowBox[{"\"\<Fragment\>\"", "\[Rule]", "None"}]}], "}"}]}], "]"}], 
     "&"}], "@", 
    RowBox[{"URLParse", "@", "url"}]}], "//", 
   "URLBuild"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"EditToExportURL", "::", "usage"}], "=", 
   "\"\<EditToExportURL[url] converts a Google Sheets spreadsheet edit URL \
into an export (i.e. download) URL by breaking the URLs into parts via \
URLParse, replacing values in the Association, and building it back up via \
URLBuild.\>\""}], ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.8244801050031905`*^9, 3.8244801089875383`*^9}, {
   3.824480314392973*^9, 3.824480403970791*^9}, {3.824480454829897*^9, 
   3.8244805659280863`*^9}, {3.824480602211356*^9, 3.8244806390902386`*^9}, {
   3.824480671520426*^9, 3.824480723995835*^9}, {3.8244807717362795`*^9, 
   3.82448085370057*^9}, 3.8244810440410304`*^9, {3.824481408837677*^9, 
   3.824481410217806*^9}, {3.82448201573048*^9, 3.8244820207429075`*^9}, {
   3.824482173537155*^9, 3.8244821789492345`*^9}, {3.82448235334713*^9, 
   3.8244824203786407`*^9}, 3.8244835749644575`*^9, {3.824491083933649*^9, 
   3.824491084293017*^9}, {3.824491133793049*^9, 3.824491139074295*^9}, {
   3.824491247013932*^9, 3.8244912869045563`*^9}},
 CellLabel->"In[1]:="]
}, Open  ]],

Cell[CellGroupData[{

Cell["Example", "Section",
 CellChangeTimes->{{3.8244813744953938`*^9, 3.824481387550578*^9}, {
