Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"terms", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"StringSplit", "[", 
         RowBox[{"#", ",", 
          RowBox[{"{", 
           RowBox[{"\"\<,\>\"", ",", "\"\<:\>\""}], "}"}]}], "]"}], 
        "\[LeftDoubleBracket]", 
        RowBox[{";;", ";;", "2"}], "\[RightDoubleBracket]"}], "&"}], "/@", 
      "#"}], "&"}], "/@", 
    RowBox[{"{", 
     RowBox[{"project", ",", "ideas", ",", "results", ",", "writing"}], 
     "}"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"terms", "=", 
   RowBox[{"terms", "\[Transpose]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"terms", "=", 
   RowBox[{"Flatten", "/@", "terms"}]}], ";"}], "\[IndentingNewLine]", 
